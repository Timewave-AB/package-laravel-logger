{
    "name": "timewave/laravel-logger",
    "version": "0.2.0",
    "description": "Alternative logger for Laravel",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Timewave\\LaravelLogger\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Timewave",
            "email": "utveckling@timewave.se"
        }
    ],
    "require": {
      "php": "^8.0",
        "guzzlehttp/promises": "2.0.4",
        "open-telemetry/exporter-otlp": "1.1.0",
        "open-telemetry/opentelemetry": "1.0.0",
        "open-telemetry/sdk": "1.1.2",
        "php-http/httplug": "2.4.1"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Timewave\\LaravelLogger\\LaravelLoggerProvider"
        ]
      }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    }
}
